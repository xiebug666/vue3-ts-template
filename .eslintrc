{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": [
    "plugin:vue/vue3-recommended",
    "eslint:recommended",
    "@vue/typescript/recommended",
    "@vue/prettier",
    "plugin:import/recommended",
    "plugin:import/typescript",
    "airbnb",
    "prettier"
  ],
  "settings": {
    "import":{
      "typescript": true,
      "node": true
    }
  },
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "prettier/prettier": ["error"],
    "vue/multi-word-component-names": "off"
  }
}
